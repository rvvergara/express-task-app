{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "consistent-return": 0,
        "no-console": 0,
        "import/no-unresolved": 0,
        "arrow-parens": 0,
        "indent": 0,
        "no-return-assign": 0,
        "func-names": 0,
        "no-use-before-define": 0
    }
}